        <!-- Single Page Header start -->
        <div class="container-fluid page-header py-5">
            <h1 class="text-center text-white display-6">Checkout</h1>
        </div>
        <!-- Single Page Header End -->

        <!-- Checkout Page Start -->
        <div class="container-fluid py-5">
            <div class="container py-5">
                <h1 class="mb-4">Billing details</h1>
                <form action="/checkout" method="post">
                    <div class="row g-5">
                        <div class="col-md-12 col-lg-6 col-xl-7">
                            <label class="form-label my-3">Address <sup>*</sup></label>
                            <div class="row">
                                <div class="col-md-6">
                                    <input type="text" name="province" class="form-control" placeholder="Province" autocomplete="off" required>
                                </div>
                                <div class="col-md-6">
                                    <input type="text" name="city" class="form-control" placeholder="City" autocomplete="off" required>
                                </div>
                            </div>                            
                            <br>
                            <input type="text" name="street" class="form-control" placeholder="Street" autocomplete="off" required>
                            <label class="form-label my-3">Notes</label>
                            <div class="form-item">
                                <textarea name="notes" class="form-control" spellcheck="false" cols="30" rows="11" placeholder="Order Notes (Optional)"></textarea>
                            </div>
                        </div>
                        <div class="col-md-12 col-lg-6 col-xl-5">
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th scope="col">Products</th>
                                            <th scope="col">Name</th>
                                            <th scope="col">Price</th>
                                            <th scope="col">Quantity</th>
                                            <th scope="col">Total</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for(let i=0; i<result.length; i++){ %>
                                        <tr>
                                            <input type="hidden" name="id_product" value="<%= result[i].id %>">
                                            <input type="hidden" name="orderquantity" value="<%= result[i].orderquantity %>">
                                            <th scope="row">
                                                <div class="d-flex align-items-center mt-2">
                                                    <img src="img/<%= result[i].filename %>" class="img-fluid rounded-circle" style="width: 90px; height: 90px;" alt="">
                                                </div>
                                            </th>
                                            <td class="py-5"><%= result[i].name %></td>
                                            <td class="py-5">Rp<%= result[i].price.toLocaleString('de-DE')%></td>
                                            <td class="py-5"><%= result[i].orderquantity%></td>
                                            <td class="py-5">Rp<%= (result[i].price * result[i].orderquantity).toLocaleString('de-DE') %></td>
                                        </tr>
                                        <% } %>

                                        <tr>
                                            <td colspan="3" class="py-5">
                                                <p>Date COD</p>
                                                <input type="datetime-local" name="dateCOD" id="choiceDateCOD" required/> 

                                                <p class="mb-0 text-dark py-4">Payment (COD Only)</p>
                                                <div class="form-check text-start">
                                                    <input type="radio" class="form-check-input bg-primary border-0" id="payment-1" name="payment" value="payment" required>
                                                    <label class="form-check-label" for="payment-1">
                                                        COD <br>
                                                        <i class="bi bi-box-seam fa-3x text-dark"></i>
                                                    </label>                                                
                                                </div>   
                                            </td>
                                        </tr>
                                        <tr>
                                            <th scope="row">
                                            </th>
                                            <td class="py-5">
                                                <p class="mb-0 text-dark text-uppercase py-3">TOTAL</p>
                                            </td>
                                            <td class="py-5"></td>
                                            <td class="py-5"></td>
                                            <td class="py-5">
                                                <div class="py-3 border-bottom border-top">
                                                    <p class="mb-0 text-dark">Rp<%= totalPrice.toLocaleString('de-DE') %></p>
                                                </div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <div class="row g-4 text-center align-items-center justify-content-center pt-4">
                                <button type="submit" class="btn border-secondary py-3 px-4 text-uppercase w-100 text-primary">Place Order</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!-- Checkout Page End -->